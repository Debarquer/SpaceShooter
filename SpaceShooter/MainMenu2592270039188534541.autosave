void DrawMainMenu(){
  background(155, 155, 155);
  PFont f;
  f = createFont("Arial", 32, true);
  textFont(f, 32);
  fill(0, 0, 0);

  String s = "Space Shooter";
  text(s, width/2 - 100, 30);

  PImage img;
  img = loadImage("Resources/ButtomTemplate.png");
  image(img, width/2 - img.width/2, 200);
}
